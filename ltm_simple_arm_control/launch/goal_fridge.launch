<launch>
	
  <node pkg="simple_arm_control" type="move_right_arm_simple" name="move_arm_to_position" output="screen">
     
     <param name="planner_service_name" value="sbpl_planning/plan_path"/>
	   <param name="goal_x" value="0.659"    type="double"/>
     <param name="goal_y" value="-0.15"  type="double"/>
	   <param name="goal_z" value="0.48"     type="double"/>

	   <param name="goal_roll" value="0.943"  type="double"/>
	   <param name="goal_yaw" value="0.126"    type="double"/>
	   <param name="goal_pitch" value="-0.768"  type="double"/>

     <param name="planning_duration" value="10.0" type="double" />
	</node>

</launch>
